<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.9.3-alpha</Version>
    <Authors>Polzoni Gilberto</Authors>
    <Description>A library to create markdown file programmatically in .NET</Description>
    <Copyright>Polzoni Gilberto</Copyright>
    <PackageTags>Markdown</PackageTags>
    <PackageLicenseFile>License.txt</PackageLicenseFile>
    <RepositoryUrl>https://github.com/GibbOne/NMarkdown</RepositoryUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/GibbOne/NMarkdown/develop/logo.ico</PackageIconUrl>
    <AssemblyVersion>0.9.3.0</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\License.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
